<%- include('./partials/header');-%>

<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
        <label for="titleNameLabel">Title</label>
        <input type="text" name="titleName" class="form-control" id="titleNameLabel">
        <label for="postBodyLabel">Post</label>
        <textarea class="form-control" name="postBody" id="postBodyLabel" cols="50" rows="10"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button> 
</form>
<%- include('./partials/footer'); -%>